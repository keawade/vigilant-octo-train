{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vigilant-octo-train",
  "version": "1.0.0",
  "main": "main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keawade/vigilant-octo-train.git"
  },
  "keywords": [],
  "author": "Keith Wade",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node --experimental-strip-types src/main.ts",
    "start:dev": "node --experimental-strip-types --env-file=.env --watch src/main.ts",
    "test": "node --experimental-strip-types --env-file=.env.test --test",
    "test:watch": "npm run test -- --watch",
    "format": "prettier . --write --list-different",
    "format:check": "prettier . --list-different",
    "lint": "eslint ."
  },
  "dependencies": {
    "fastify": "^5.2.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.8",
    "eslint": "^9.21.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.1-alpha.18"
  },
  "engines": {
    "node": ">=22.6"
  }
}
